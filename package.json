{
  "name": "vue-vuetify-material-you-template",
  "version": "0.0.1",
  "private": true,
  "author": "gumengyu",
  "description": "vue-vuetify-material-you-template",
  "scripts": {
    "preview": "vite preview",
    "dev": "vite",
    "build": "vite build",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:ts",
    "lint:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx,vue}\" --fix",
    "lint:prettier": "prettier --write \"src/**/*.{js,tsx,css,scss,vue,md,json,html}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.2.0",
    "@mdi/js": "^6.5.95",
    "@vueuse/components": "9.8.1",
    "@vueuse/core": "9.8.1",
    "axios": "^1.2.6",
    "dayjs": "^1.11.1",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.9.2",
    "md5": "^2.3.0",
    "pinia": "^2.0.13",
    "vue": "^3.2.41",
    "vue-content-loader": "^2.0.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.2.1",
    "vuetify-ctx-menu": "git+https://github.com/GuMengYu/vue3-context-menu.git#v0.0.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/md5": "^2.3.2",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.7.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.38.0",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.3",
    "vite-plugin-resolve": "^2.3.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.0.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
